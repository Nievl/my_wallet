  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to Node",
      "type": "node",
      "request": "attach",      
      "localRoot": "${workspaceFolder}",
      "port": 9229,
      "address": "0.0.0.0",
      "protocol": "inspector",
      "restart": true
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Node: Nodemon",
      "processId": "${command:PickProcess}",
      "restart": true,
      "protocol": "inspector"
    }
  ]
}
